{
    "name": "php-telegram-bot/workspace",
    "description": "Workspace for development and tests",
    "type": "project",
    "authors": [
        {
            "name": "Tii",
            "email": "mail@tii.one"
        }
    ],
    "require": {
        "longman/telegram-bot": "@dev",
        "vlucas/phpdotenv": "5.4.x-dev"
    },
    "autoload": {
        "files": [
            "helpers.php"
        ],
        "psr-4": {
            "Longman\\TelegramBot\\Commands\\": "app/Telegram/Commands",
            "App\\": "app"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "./packages",
            "options": {
                "symlink": true
            }
        }
    ]
}
